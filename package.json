{
  "name": "web-tooling-benchmark",
  "description": "JavaScript benchmark for common web developer workloads",
  "version": "0.5.3",
  "repository": "https://github.com/v8/web-tooling-benchmark",
  "main": "src/cli.js",
  "scripts": {
    "benchmark": "node dist/cli.js",
    "build:uglify-js-bundled": "node node_modules/uglify-js/bin/uglifyjs -b preamble=\"'const UglifyJS = module.exports = {};'\" --self > build/uglify-js-bundled.js",
    "build": "webpack",
    "postinstall": "npm run build:uglify-js-bundled && npm run build",
    "test": "vitest run --globals",
    "update-lock": "npm install --package-lock",
    "new-benchmark": "wtb-generate",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.css": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "webpack.config.js": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "author": {
    "name": "Benedikt Meurer",
    "email": "bmeurer@chromium.org",
    "url": "http://benediktmeurer.de"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/parser": "7.22.7",
    "@babel/standalone": "7.22.8",
    "acorn": "8.10.0",
    "acorn-walk": "8.2.0",
    "autoprefixer": "10.4.14",
    "babel-minify": "0.5.2",
    "benchmark": "^2.1.4",
    "buble": "0.20.0",
    "chai": "4.3.7",
    "coffeescript": "2.7.0",
    "compute-gmean": "^1.1.0",
    "espree": "9.6.1",
    "esprima": "4.0.1",
    "jshint": "2.13.6",
    "lebab": "3.2.1",
    "memfs": "^4.2.0",
    "postcss": "8.4.27",
    "postcss-nested": "6.0.1",
    "prepack": "0.2.54",
    "prettier": "3.0.0",
    "source-map": "0.6.1",
    "terser": "5.19.2",
    "typescript": "5.1.6",
    "uglify-js": "3.17.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "node-libs-browser": "^2.2.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "semver": "^7.5.3",
    "stream-browserify": "^3.0.0",
    "vitest": "^0.33.0",
    "web-tooling-benchmark-generator": "^1.0.2",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "magic-string": "0.30.1",
    "node-gyp": "^9.4.0",
    "yargs-parser": "^21.1.1",
    "fbjs": "^3.0.5",
    "zip-dir": "2.0.0"
  }
}
