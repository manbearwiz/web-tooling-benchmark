{
  "name": "web-tooling-benchmark",
  "description": "JavaScript benchmark for common web developer workloads",
  "version": "0.5.3",
  "repository": "https://github.com/v8/web-tooling-benchmark",
  "main": "src/cli.js",
  "scripts": {
    "benchmark": "node dist/cli.js",
    "build:terser-bundled": "node node_modules/terser/bin/uglifyjs -b preamble=\"'const Terser = module.exports = {};'\" --self > build/terser-bundled.js",
    "build:uglify-js-bundled": "node node_modules/uglify-js/bin/uglifyjs -b preamble=\"'const UglifyJS = module.exports = {};'\" --self > build/uglify-js-bundled.js",
    "build": "NODE_OPTIONS=--openssl-legacy-provider webpack",
    "postinstall": "npm run build:terser-bundled && npm run build:uglify-js-bundled && npm run build",
    "precommit": "node tools/hooks/pre-commit.js && lint-staged",
    "test": "jest",
    "update-lock": "npm install --package-lock",
    "new-benchmark": "wtb-generate"
  },
  "lint-staged": {
    "src/**/*.css": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "webpack.config.js": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "Chrome 50",
    "Firefox 50"
  ],
  "author": {
    "name": "Benedikt Meurer",
    "email": "bmeurer@chromium.org",
    "url": "http://benediktmeurer.de"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/parser": "7.22.5",
    "@babel/standalone": "7.22.5",
    "acorn": "6.4.2",
    "acorn-walk": "^6.2.0",
    "autoprefixer": "8.6.5",
    "babel-minify": "0.4.3",
    "benchmark": "^2.1.4",
    "buble": "0.20.0",
    "chai": "4.1.2",
    "coffeescript": "2.7.0",
    "compute-gmean": "^1.1.0",
    "espree": "4.1.0",
    "esprima": "4.0.1",
    "jshint": "2.13.6",
    "lebab": "3.2.1",
    "postcss": "7.0.39",
    "postcss-nested": "4.2.3",
    "prepack": "0.2.25",
    "prettier": "2.8.8",
    "source-map": "0.6.1",
    "string-align": "^0.2.0",
    "terser": "3.8.2",
    "typescript": "4.9.5",
    "uglify-js": "3.17.4",
    "virtualfs": "^2.2.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.4.1",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^8.0.3",
    "jest": "^22.4.4",
    "lint-staged": "^13.2.3",
    "raw-loader": "github:bmeurer/raw-loader#escape",
    "semver": "^7.5.3",
    "web-tooling-benchmark-generator": "^1.0.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.10.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
